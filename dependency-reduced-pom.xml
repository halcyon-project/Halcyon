          <plugin>
            <artifactId>maven-shade-plugin</artifactId>
            <executions>
              <execution>
                <phase>package</phase>
                <goals>
                  <goal>shade</goal>
                </goals>
                <configuration>
                  <transformers>
                    <transformer>
                      <resource>META-INF/spring.handlers</resource>
                    </transformer>
                    <transformer>
                      <resource>META-INF/spring.factories</resource>
                    </transformer>
                    <transformer>
                      <resource>META-INF/spring.schemas</resource>
                    </transformer>
                    <transformer />
                    <transformer>
                      <manifestEntries>
                        <Multi-Release>true</Multi-Release>
                      </manifestEntries>
                      <mainClass>com.ebremer.halcyon.converters.NS2OA</mainClass>
                    </transformer>
                  </transformers>
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
